# AWS CloudFront Entity Test Environment Variables
# Copy this file to .env and fill in your actual values

# AWS Credentials (if not using IAM roles or AWS CLI profiles)
# Note: The entity uses Monk's built-in AWS integration, these are only needed if running tests outside Monk
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_REGION=us-east-1

# Test Configuration
# AWS region for CloudFront operations (CloudFront is global but API calls go to us-east-1)
TEST_REGION=us-east-1

# Optional: Custom test domain for origin testing
# TEST_ORIGIN_DOMAIN=example.com

# Test timeout settings (in milliseconds)
# Distribution deployment can take 15-20 minutes
TEST_TIMEOUT=1800000

# Optional: Test tags
TEST_ENVIRONMENT=test
TEST_PROJECT=cloudfront-entity-test

# CloudFront specific settings
# Price class for test distributions
TEST_PRICE_CLASS=PriceClass_100

# Cache behavior settings for testing
TEST_DEFAULT_TTL=86400
TEST_MAX_TTL=31536000

# Invalidation testing
TEST_INVALIDATION_PATHS="[\"/*\"]"
TEST_BATCH_INVALIDATION_PATHS="[\"/index.html\", \"/css/*\", \"/js/*\"]"
