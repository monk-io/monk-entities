# Redis Cloud Test Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Redis Cloud API User Key
# Get this from https://app.redislabs.com/#/admin/settings/access_control/api_keys
REDIS_CLOUD_USER_KEY=your-redis-cloud-user-key-here

# Required: Redis Cloud API Account Key  
# Get this from https://app.redislabs.com/#/admin/settings/access_control/api_keys
REDIS_CLOUD_ACCOUNT_KEY=your-redis-cloud-account-key-here

# Optional: Test configuration
MONKEC_VERBOSE=true
# TEST_TIMEOUT=540000 