# DigitalOcean App Platform Functions Test Environment Variables
# Copy this file to .env and fill in your actual values

# Required: DigitalOcean API Token
# Get from: https://cloud.digitalocean.com/account/api/tokens
DO_API_TOKEN=your_digitalocean_api_token_here

# Optional: Test secret for environment variable testing
TEST_SECRET=test-secret-value

# Optional: Test configuration
MONKEC_VERBOSE=true
TEST_TIMEOUT=300000

# Optional: Log forwarding tokens (if testing log destinations)
LOGTAIL_TOKEN=your_logtail_token_here
DATADOG_API_KEY=your_datadog_api_key_here