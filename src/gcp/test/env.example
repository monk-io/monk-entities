# GCP Entities Test Environment Variables
#
# Copy this file to .env and fill in your values:
#   cp env.example .env
#
# The test framework automatically loads .env files from the test directory.

# Required: GCP Project ID
# The project where test resources will be created
GCP_PROJECT_ID=your-gcp-project-id

# Optional: GCP Region for resources
# Default: us-central1
GCP_REGION=us-central1

# Optional: Unique suffix for resource names to avoid conflicts
# Default: random string
TEST_SUFFIX=test123

# Optional: Enable verbose test output
MONKEC_VERBOSE=true

# Optional: Test timeout in milliseconds
TEST_TIMEOUT=600000
