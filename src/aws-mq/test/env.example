# AWS MQ Entity Integration Test Environment Variables
# Copy this file to .env and fill in your actual values

# AWS Configuration (usually handled by AWS CLI or IAM roles)
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your-access-key-id
# AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Test Configuration
TEST_TIMEOUT=1800000
MONKEC_VERBOSE=true

# MQ Broker Passwords (16+ characters required by AWS)
# These will be set as Monk secrets during test execution
# MQ_ADMIN_PASSWORD=your-secure-admin-password-here-16chars
# MQ_USER_PASSWORD=your-secure-user-password-here-16chars

# Test Environment Tags
TEST_ENVIRONMENT=integration-test
TEST_PURPOSE=monk-entity-testing