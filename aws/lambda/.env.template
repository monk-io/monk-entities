# AWS Credentials - REPLACE WITH YOUR ACTUAL VALUES
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# Lambda Function Configuration
LAMBDA_FUNCTION_NAME=docker-test-function
LAMBDA_RUNTIME=python3.9
LAMBDA_ROLE=arn:aws:iam::YOUR_ACCOUNT_ID:role/YOUR_LAMBDA_ROLE
LAMBDA_HANDLER=test_lambda_function.lambda_handler
LAMBDA_DESCRIPTION=Test function deployed via Docker container
LAMBDA_TIMEOUT=30
LAMBDA_MEMORY_SIZE=128
LAMBDA_TAGS=Environment=test,DeployedBy=docker-container
