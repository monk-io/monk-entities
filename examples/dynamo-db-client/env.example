# DynamoDB Client Configuration
# Copy this file to .env and update with your actual values

# Required: DynamoDB Table Name
DYNAMODB_TABLE_NAME=users-table

# Optional: AWS Region (defaults to us-east-1)
AWS_REGION=us-east-1

# Optional: Client operation configuration
OPERATION_INTERVAL_MS=3000
MAX_OPERATIONS=50

# AWS Credentials (usually handled by IAM roles or environment)
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key 